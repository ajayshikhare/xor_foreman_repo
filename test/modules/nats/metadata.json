{
  "name": "ripienaar-nats",
  "version": "0.0.5",
  "author": "ripienaar",
  "summary": "NATS.io module for AIO Puppet 4",
  "license": "Apache-2.0",
  "source": "https://github.com/ripienaar/puppet-nats",
  "project_page": "https://github.com/ripienaar/puppet-nats",
  "issues_url": "https://github.com/ripienaar/puppet-nats/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.12.0 < 5.0.0"},
    {"name":"camptocamp/systemd","version_requirement":">= 0.3.0 < 1.0.0"}
  ],
  "data_provider": "hiera",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "4.5.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
